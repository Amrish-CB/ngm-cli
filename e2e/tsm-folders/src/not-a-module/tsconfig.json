{
  "compilerOptions": {
    "outDir": "../../dist/not-a-module",
    "module": "commonjs",
    "target": "es5",
    "sourceMap": false,
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "stripInternal": true,
    "lib": ["dom", "es6"]
  },
  "exclude": [
    "node_modules"
  ]
}